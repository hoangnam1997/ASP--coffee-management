<!-- Modal -->
<div class="modal fade" id="deleteShopModal" tabindex="-1" role="dialog" aria-labelledby="deleteShopModalAria" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Are you sure to delete this shop?</h4>
            </div>
            <div class="modal-body">
                Click "Yes" to delete, click "No" to cancel.
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">No</button>
                <button type="button" class="btn btn-danger" onclick="DeleteShop()">Yes</button>
            </div>
        </div>
    </div>
</div>